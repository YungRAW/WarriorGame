{
    "require": {
        "php": ">=7.4.14",
        "symfony/yaml": "^5.2"
      },
      "require-dev": {
        "phpunit/phpunit": "^9.5"
      },
      "config": {
        "optimize-autoloader": true,
        "preferred-install": {
          "*": "dist"
        },
        "sort-packages": true
      },
      "autoload": {
        "psr-4": {
          "Razvan\\Emagia\\": "src/"
        }
      },
      "autoload-dev": {
        "psr-4": {
          "Razvan\\Emagia\\Tests\\": "tests/"
        }
      }

}